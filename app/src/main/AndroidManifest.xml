<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jason.remotecamera_wja">

    <uses-feature android:name="android.hardware.camera" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <!-- 写sd卡的权限 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!-- 读sd卡权限 -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".InitApp"
        android:allowBackup="true"
        android:icon="@mipmap/camera80"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/camera80"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".parta.PartAMain" />
        <activity android:name=".partb.PartBMain" />
        <activity android:name=".camera.CameraActivity" />
        <activity android:name=".socketTest.SocketA" />
        <activity android:name=".socketTest.SocketB" />
        <activity android:name=".parta.pictures.PicturesAll" />
        <activity android:name=".parta.pictures.PicturesDetail" />
        <activity android:name=".partb.ControllActivity"></activity>
    </application>

</manifest>